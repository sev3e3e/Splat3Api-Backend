{
    "type": "module",
    "name": "splat3api-backend",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "author": "medi",
    "license": "MIT",
    "private": false,
    "scripts": {
        "dev": "node --loader ts-node/esm ./src/index.ts",
        "devfn": "functions-framework --target=index --signature-type=event",
        "build": "tsc",
        "deploy": "tsc && gcloud functions deploy index --runtime nodejs16 --trigger-topic=splat3 --env-vars-file prod.env.yaml --region=asia-northeast1",
        "test": "vitest"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "^3.1.3",
        "@google-cloud/logging-winston": "^5.3.0",
        "async-retry": "^1.3.3",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "nxapi": "1.6.1",
        "redis": "4.5.1",
        "splatnet3-types": "0.2.20230227204004",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@google-cloud/pubsub": "^3.3.0",
        "@sev3e3e/splat3api-client": "0.0.18-alpha",
        "@types/async-retry": "^1.4.5",
        "@types/node": "^18.11.18",
        "@types/redis-mock": "^0.17.1",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "@vitest/ui": "^0.28.4",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.3",
        "redis-mock": "^0.56.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "vite": "^4.1.1",
        "vitest": "^0.28.4"
    }
}
